{
  "name": "open-by-clipboard",
	"displayName": "Open files by clipboard",
	"description": "Open file(s), from copied text.",
	"version": "1.0.2",
	"repository": "https://github.com/kuone314/VSCodeOpenFilesFromClipboard",
	"publisher": "kuone",
  "icon": "doc/icon.png",
	"engines": {
		"vscode": "^1.71.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
        "onCommand:open-files-from-clipboard"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "open-files-from-clipboard",
				"title": "Open files from clipboard."
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.71.0",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.0",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^5.16.0",
		"@typescript-eslint/parser": "^5.16.0",
		"eslint": "^8.11.0",
		"glob": "^7.2.0",
		"mocha": "^9.2.2",
		"typescript": "^4.5.5",
		"@vscode/test-electron": "^2.1.3"
	}
}
